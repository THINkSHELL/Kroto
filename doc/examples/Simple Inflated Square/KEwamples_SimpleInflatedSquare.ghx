                  <items count="10">
                    <item name="CodeInput" type_name="gh_string" type_code="10">"""
Define a problem for the Kroto solver.
"""

import Grasshopper as gh
import sys
import os

ghcompfolder = gh.Folders.DefaultAssemblyFolder
krotofolder = os.path.join(ghcompfolder, 'Kroto')
if krotofolder not in sys.path:
    sys.path.append(krotofolder)

import rhinoscriptsyntax as rs
import copy

prev = rs.EnableRedraw(False)

class KMesh:
    """
    Custom type to pass the mesh problem to the solver
    """

    def __init__(self, mesh, KEdges, q_faces, pressure, density, reference):
        if KEdges:
            self.cables = KEdges.cables
            self.fixed = KEdges.fixed
            self.q_faces = q_faces
            self.ql = KEdges.ql
            self.q_cables = KEdges.q_cables
            self.n_cable = KEdges.n_cable
        else:
            self.cables = None
            self.fixed = None
            self.q_faces = None
            self.ql = None
            self.q_cables = None
            self.n_cable = None
        self.pressure = pressure
        self.density = density
        self.mesh = mesh
        self.reference = reference

    def __str__(self):
        """Simple display for error checking"""
        str = ''
        for key in self.__dict__:
            str += "%s: %s\n" % (key, self.__dict__[key])
        return str


# Create defaults (= 1) for the surface density
if not q_faces:
    q_faces = [1 for i in range(rs.MeshFaceCount(mesh))]
elif len(q_faces) == 1:
    q_faces = [q_faces[0] for i in range(rs.MeshFaceCount(mesh))]
# Pressure and density defaults to 0
if not pressure: pressure = 0
if not density: density = 0
# Create defaults (= current mesh) for the reference mesh
if not reference: reference = rs.MeshVertices(mesh)

KMesh = KMesh(mesh, KEdges, q_faces, pressure, density, reference)

rs.EnableRedraw(prev)</item>
