                    <item name="CodeInput" type_name="gh_string" type_code="10">#!/usr/bin/env python
# -*- coding: utf-8 -*-

"""
The Kroto solver.
"""

import Grasshopper as gh
import sys
import os

ghcompfolder = gh.Folders.DefaultAssemblyFolder
krotofolder = os.path.join(ghcompfolder, 'Kroto')
if krotofolder not in sys.path:
    sys.path.append(krotofolder)

import meshminimize as mm
import rhinoscriptsyntax as rs
import copy
from imp import reload
reload(mm)

prev = rs.EnableRedraw(False)

# Useful value
zero_vector = rs.VectorCreate(rs.AddPoint(0, 0, 0), rs.AddPoint(0, 0, 0))

if state in ['step', 'run']:
    # Remind GHPython where we are, sometimes it forgets
    cur_mesh = rs.AddMesh(vertices, rs.MeshFaceVertices(KMesh.mesh))
    # Update KOptions to the MeshMinimize module
    if KOptions:
        for key in KOptions.__dict__:
            if KOptions.__dict__[key]:
                mm.__dict__[str.upper(key)] = KOptions.__dict__[key]
    # Short-hand for the solve operation
    def solve():
        return mm.minimize_mesh(
            cur_mesh,
            KMesh.cables, 
            fixed=KMesh.fixed,
            qs=cur_qs,
            q_cables=KMesh.q_cables, 
            reference=KMesh.reference, 
            ql=KMesh.ql, 
            n_cable=KMesh.n_cable,
            p=KMesh.pressure,
            g=KMesh.density
        )
print KMesh.density
if state == 'init':
    # Initialize current state information
    step = 0
    step_qs = 0
    ran = False

    # Pass KOptions to the MeshMinimize module
    if KOptions:
        for key in KOptions.__dict__:
            if KOptions.__dict__[key]:
                mm.__dict__[str.upper(key)] = KOptions.__dict__[key]

    # Initialize inputs
    vertices = rs.MeshVertices(KMesh.mesh)
    cur_mesh = KMesh.mesh
    cur_qs = copy.deepcopy(KMesh.q_faces)

    # Initialize outputs
    mesh_out = rs.AddMesh(vertices, rs.MeshFaceVertices(KMesh.mesh))
    displacements = [zero_vector for i in range(len(vertices))]
    out = None

elif state == 'step':
    # Running step-by-step. We need to take care of the loops controls
    # ourselves: use the meshminimize options to stop it. KOptions will
    # keep the original values.
    if run:
        if KOptions.max_iter - step:
            mm.MAX_ITER = 1
            mm.MAX_ITER_QS = 1
            step += 1
            vertices, out = solve()
        elif KOptions.max_iter_qs - step_qs:
            mm.MAX_ITER = 0
            mm.MAX_ITER_QS = 2
            step_qs += 1
            step = 0
            vertices, out = solve()
        else:
            mm.MAX_ITER = 0
            mm.MAX_ITER_QS = 1
            vertices, out = solve()
        ran = True

elif state == 'run':
    if run:
        vertices, out = solve()
        ran = True

# Write ouptuts
if (state in ['step', 'run']) and ran:
    mesh_out = rs.AddMesh(vertices, rs.MeshFaceVertices(KMesh.mesh))
    displacements = [
        rs.VectorCreate(v1, v2)
        for (v1, v2) in zip(rs.MeshVertices(KMesh.mesh), vertices)
    ]
    stresses = out['S']
    convergence = out['convergence']
    if mm.SAVE_RESULTS:
        F_membrane = out['M']
        F_cable = out['C']
        F_pressure = out['P']
        F_gravity = out['G']
        area = out['A']

rs.EnableRedraw(prev)</item>
